extends layout

block content
  // ======= Hero Section =======
  section#hero.d-flex.align-items-center
    .container-fluid(data-aos='fade-up')
      .row.justify-content-center
        .col-xl-5.col-lg-6.pt-3.pt-lg-0.order-2.order-lg-1.d-flex.flex-column.justify-content-center
          h1 Algoritm bilan tezkor-samarali loyihalar
          h2 Sizning orzularingizni amalga oshirish uchun bizning dasturchilarimiz hamisha tayyor!
          div
            a.btn-get-started.scrollto(href='#about') Boshlamoq
        .col-xl-4.col-lg-6.order-1.order-lg-2.hero-img(data-aos='zoom-in', data-aos-delay='150')
          img.img-fluid.animated(src='images/img/hero-img.png', alt='')
  // End Hero
  main#main
    // ======= About Section =======
    section#about.about
      .container
        .row
          .col-lg-6.order-1.order-lg-2(data-aos='zoom-in', data-aos-delay='150')
            img.img-fluid(src='images/img/about.jpg', alt='')
          .col-lg-6.pt-4.pt-lg-0.order-2.order-lg-1.content(data-aos='fade-right')
            h3 Bizni tanlaganlar nimalarga erishadi
            p.font-italic
              |Bizning sizga maxsus takliflarimiz.......
            ul
              li
                i.icofont-check-circled
                |  Shamoldek-tez, sifatli va betakror dizaynlar;
              li
                i.icofont-check-circled
                |  O`z biznesingizni uydan chiqmasdan nazorat qilish
              li
                i.icofont-check-circled
                | Loyihalaringizni rivojlantirish va dunyo miqiyosiga olib chiqish
            a.read-more(href='#')
              | Batafsil
              i.icofont-long-arrow-right
    // End About Section
    // ======= Counts Section =======
    section#counts.counts
      .container
        .row.counters
          .col-lg-3.col-6.text-center
            span(data-toggle='counter-up') 232
            p Foydalanuvchilar
          .col-lg-3.col-6.text-center
            span(data-toggle='counter-up') 521
            p Loyihalar
          .col-lg-3.col-6.text-center
            span(data-toggle='counter-up') 1,463
            p Yangi dasturlar
          .col-lg-3.col-6.text-center
            span(data-toggle='counter-up') 15
            p Jamoamiz
